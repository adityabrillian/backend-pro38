<!-- Modal -->
<div
  class="modal fade"
  id="addModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="addModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="addModalLabel">Tambah Bank</h5>
        <button
          type="button"
          class="close"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <form action="/admin/bank" method="POST" enctype="multipart/form-data">
        <div class="modal-body">
          <div class="form-group">
            <label for="nameBank" class="col-form-label">Nama Bank</label>
            <input
              required
              name="nameBank"
              type="text"
              class="form-control"
              placeholder="Isi nama bank"
            />
          </div>
          <div class="form-group">
            <label for="nomorRekening" class="col-form-label"
              >Nomor Rekening</label
            >
            <input
              required
              name="nomorRekening"
              type="text"
              class="form-control"
              placeholder="Isi nomor rekening"
            />
          </div>
          <div class="form-group">
            <label for="name" class="col-form-label">Nama Pemilik</label>
            <input
              required
              name="name"
              type="text"
              class="form-control"
              placeholder="Isi atas nama pemilik rekening"
            />
          </div>
          <div class="form-group">
            <label for="image" class="col-form-label">Image</label>
            <input required name="image" type="file" class="form-control" />
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">
            Tutup
          </button>
          <button type="submit" class="btn btn-primary">Tambah</button>
        </div>
      </form>
    </div>
  </div>
</div>
